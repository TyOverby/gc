// Integers

assert 1 == 1;
assert 1 != 2;
assert 1 < 2;
assert 2 > 1;
assert 1 <= 2;
assert 2 <= 2;
assert 2 >= 1;
assert 2 >= 2;

assert ! (1 != 1);
assert ! (1 == 2);
assert ! (2 < 1);
assert ! (1 > 2);
assert ! (2 <= 1);
assert ! (1 >= 2);

assert +7 == 7;
assert -7 == 0-7;
assert 7+8 == 15;
assert 7*8 == 56;
assert 3*5 == 15;
assert 23/5 == 4;
assert 23%5 == 3;


// Floats

assert 1.0 == 1.0;
assert 1.0 != 2.0;
assert 1.0 < 2.0;
assert 2.0 > 1.0;
assert 1.0 <= 2.0;
assert 2.0 <= 2.0;
assert 2.0 >= 1.0;
assert 2.0 >= 2.0;

assert ! (1.0 != 1.0);
assert ! (1.0 == 2.0);
assert ! (2.0 < 1.0);
assert ! (1.0 > 2.0);
assert ! (2.0 <= 1.0);
assert ! (1.0 >= 2.0);

assert +7.0 == 7.0;
assert -7.0 == 0.0-7.0;
assert 7.0+8.0 == 15.0;
assert 7.5*8.0 == 60.0;
assert 3.5*5.0 == 17.5;
assert 24.0/5.0 == 4.8;

4;
