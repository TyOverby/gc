// Literals

let t1 = "";
let foo = "foo";
let barb = "barb";


// Length

assert #t1 == 0;
assert #foo == 3;
assert #barb == 4;


// Indexing

assert foo[0] < foo[1];
assert foo[1] == foo[2];


// Concatenation

let foobarb = foo # barb;
let barbfoo = barb # foo;

assert #foobarb == 7;
assert #barbfoo == 7;
assert foobarb[0] == "f"[0];
assert foobarb[6] == "b"[0];
assert barbfoo[0] == "b"[0];
assert barbfoo[6] == "o"[0];
